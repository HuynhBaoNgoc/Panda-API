{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/MyLog-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "DatabaseOptions": {
    "Database": "pandadb",
    "Server": "localhost",
    "Port": "3306",
    "User": "root",
    "Password": "root",
    "Options": "SslMode=None;AllowUserVariables=True;Allow Zero Datetime=True;"
  }
}
